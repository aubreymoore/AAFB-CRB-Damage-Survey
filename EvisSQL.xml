 <query>
   <shortdescription>Tree inspection</shortdescription>
   <description>Tree inspection</description>
   <databasetype>MYSQL</databasetype>
   <databasehost>mysql.guaminsects.net</databasehost>
   <databaseport>3306</databaseport>
   <databasename>oryctes</databasename>
   <databaseusername>readonlyguest</databaseusername>
   <databasepassword>readonlypassword</databasepassword>
   <sqlstatement>SELECT * FROM crb_tree_inspection
   WHERE latitude BETWEEN 13.2285 AND 13.6611
   AND longitude BETWEEN 144.6134 AND 144.9643</sqlstatement>
   <autoconnect>true</autoconnect>
 </query>
